# LFS vector image assets 
*.svg filter=lfs diff=lfs merge=lfs -text
*.ai filter=lfs diff=lfs merge=lfs -text
*.pdf filter=lfs diff=lfs merge=lfs -text

# LFS raster image assets 
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.tiff filter=lfs diff=lfs merge=lfs -text

# LFS amadine and procreate image editor project files
*.amdc filter=lfs diff=lfs merge=lfs -text
*.procreate filter=lfs diff=lfs merge=lfs -text

# LFS silhouette studio project files
*.studio3 filter=lfs diff=lfs merge=lfs -text

# LFS GeoCart and QGIS project files
*.geo3 filter=lfs diff=lfs merge=lfs -text
*.qgz filter=lfs diff=lfs merge=lfs -text